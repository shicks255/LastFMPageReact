{
  "name": "lastfm_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "patch": "node ./scripts/patch-react-spring.js",
    "build": "craco build",
    "test": "craco test --runInBand",
    "test-ci": "npm test -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "install": "copyfiles -u 1 ./build/* ./build/static/css/* ./build/static/js/* \\\\thinkcentre\\caddy\\lastFm\\"
  },
  "dependencies": {
    "@nivo/bump": "0.79.1",
    "@nivo/calendar": "0.79.1",
    "@nivo/core": "0.79.0",
    "@nivo/line": "0.79.1",
    "@nivo/radar": "0.79.1",
    "@nivo/sunburst": "0.79.1",
    "@nivo/treemap": "0.79.1",
    "copyfiles": "^2.4.1",
    "nivo": "0.31.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "query-string": "^7.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "~3.6.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.2.0",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "^17.0.33",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "autoprefixer": "10.4.2",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.2",
    "eslint-plugin-react-hooks": "4.3.0",
    "msw": "0.38.1",
    "postcss": "8.4.7",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.23",
    "typescript": "4.5.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/src/$1"
    },
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 45,
        "lines": 45,
        "statements": 45
      }
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!src/types/**/*.{ts,tsx}"
    ]
  }
}
